{
    "ai": {
        "name": "General Purpose AI Assistant",
        "module_name": "ai.general_ai",
        "class_name": "GeneralAI",
        "llm": {
            "name": "OpenAI LLM",
            "module_name": "llms.open_ai.openai_llm",
            "class_name": "OpenAILLM",
            "arguments": {
                "model": "gpt-3.5-turbo-0613",
                "temperature": 0.5,
                "max_supported_tokens": 4096,
                "conversation_history": true,
                "trim_conversation_history_at_token_count": 3000,
                "system_prompt": "You are a friendly digital assistant.  You can help me with my work, or just chat with me.",
                "include_system_info": true
            }
        },
        "subordinate_ais": [],
        "tools": [
            {
                "name": "Get Memory Tool",
                "module_name": "tools.memory.memory_tool",
                "class_name": "MemoryTool",
                "function_name": "get_memory",
                "arguments": {
                    "db_env_location": "src/db/database/db.env"
                }
            }
        ]
    },
    "runners": [
        {
            "name": "Console Runner",
            "module_name": "runners.console.console_runner",
            "class_name": "ConsoleRunner",
            "arguments": {
                "user_information": {
                    "user_name": "Aron",
                    "email_address": "aronweiler@gmail.com",
                    "location": "San Diego, CA"
                }
            }
        }
    ]
}