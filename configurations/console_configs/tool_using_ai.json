{
    "name": "General Purpose AI Assistant",
    "module_name": "ai.general_ai",
    "class_name": "GeneralAI",
    "store_conversation_history": true,
    "db_env_location": "src/db/database/db.env",
    "system_prompts": [
        "You are a digital assistant named Jarvis, who's job it is to help the user with any questions they might have, or just chat with them.",
        "Answer all queries as briefly and concisely as you can."
    ],
    "include_system_info": true,
    "llm": {
        "name": "OpenAI LLM",
        "module_name": "llms.open_ai.langchain_llm_chain",
        "class_name": "LangChainLLMChainWithEntityMemory",
        "arguments": {
            "model": "gpt-3.5-turbo-0613",
            "temperature": 0.5,
            "max_supported_tokens": 4096,
            "max_completion_tokens": 1096,
            "max_function_limit": 10,
            "db_env_location": "src/db/database/db.env"
        }
    },
    "subordinate_ais": [
        "configurations/console_configs/tool_using_ai.json"
    ],
    "tools": "configurations/tools/all_tools.json"
}