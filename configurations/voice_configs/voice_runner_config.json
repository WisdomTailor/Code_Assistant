{
    "ai": {
        "name": "General Purpose AI Assistant",
        "module_name": "ai.general_ai",
        "class_name": "GeneralAI",
        "store_conversation_history": true,
        "db_env_location": "src/db/database/db.env",
        "system_prompts": [
            "You are a digital assistant named Jarvis, who's job it is to help the user with any questions they might have.",
            "If the user is asking a question in reference to something that might have been in a previous conversation, use the 'search_for_conversation' function to search for it.",
            "Evaluate all of the user's statements and questions for the possibility of using a function that you know about to resolve the query.",
            "Only use the tools and functions that you have available to you, never make up new ones!",
            "Answer all queries as briefly and concisely as you can, do not add anything to the answers (such as the function you used to get the answer), just answer the user's query directly!"
        ],
        "include_system_info": true,
        "llm": {
            "name": "OpenAI LLM",
            "module_name": "llms.open_ai.langchain_llm_chain",
            "class_name": "LangChainLLMChain",
            "arguments": {
                "model": "gpt-3.5-turbo-0613",
                "temperature": 0.5,
                "max_supported_tokens": 4096,
                "max_completion_tokens": 1096,
                "max_function_limit": 10,
                "db_env_location": "src/db/database/db.env"
            }
        },
        "subordinate_ais": [],
        "tools": "configurations/tools/all_tools.json"
    },
    "runners": [
        {
            "name": "Console Runner",
            "module_name": "runners.voice.voice_runner",
            "class_name": "VoiceRunner",
            "arguments": {
                "activation_cooldown": 2,
                "mute_while_listening": false,
                "save_audio": false,
                "sts_model": "tiny.en",
                "default_tts_voice": "Matthew",
                "db_env_location": "src/db/database/db.env",
                "top_k": 10,
                "wake_word_models": [
                    {
                        "model_name": "hey_jarvis_v0.1",
                        "model_path": "C:\\Repos\\DocTalk\\src\\runners\\voice\\models\\hey_jarvis_v0.1.onnx",
                        "training_data": "src/runners/voice/training/aron/model.pkl",
                        "user_information": {
                            "user_name": "Aron Weiler",
                            "user_location": "San Diego",
                            "user_age": 44,
                            "user_email": "aronweiler@gmail.com",
                            "settings": {
                                "personality_keywords": "serious, professional, friendly",
                                "tts_voice": "Brian",
                                "speech_rate": 150
                            }
                        }
                    }                    
                ]
            }
        }
    ]
}